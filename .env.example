# Variables de entorno para Haizea-Llevant
# Copia este archivo como .env y modifica los valores según sea necesario

# Puerto del servidor (por defecto: 3000)
PORT=3000

# Entorno de ejecución
NODE_ENV=production

# Zona horaria
TZ=Europe/Madrid

# Configuración de logs (opcional)
LOG_LEVEL=info

# Directorio de datos (opcional)
DATA_DIR=./data